{
  "name": "lloadout/mysqlcompare",
  "description": "mysqlcompare is a commandline tool to compare mysql databases",
  "type": "library",
  "license": "MIT",
  "bin": [
    "mysqlcompare"
  ],
  "require": {
    "illuminate/filesystem": "^8.61",
    "illuminate/support": "^8.61",
    "symfony/console": "^5.3",
    "symfony/process": "^5.3",
    "symfony/finder": "^5.3",
    "nunomaduro/termwind": "^1.0",
    "doctrine/dbal": "^3.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.1",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "LLoadout\\MysqlCompare\\": "src/"
    }
  },
  "authors": [
